{
  "name": "sync",
  "description": "sync your project codes to remote server",
  "version": "0.0.1",
  "publisher": "Plutonist",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.",
    "workspaceContains:.vscode-sync.json"
  ],
  "main": "./out/src/Extension",
  "contributes": {
    "commands": [
      {
        "command": "Sync.Init",
        "title": "Sync: Init"
      },{
        "command": "Sync.UploadAll",
        "title": "Sync: Upload All"
      },{
        "command": "Sync.DownloadAll",
        "title": "Sync: Download All"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-plugin-transform-es2015-spread": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "babel-preset-stage-3": "^6.1.18",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "ssh2": "^0.4.12"
  }
}
